#include <iostream> 
#include <cstdlib> 
 using namespace std;  
int b = 50;    
namespace N1  
{     
int a=80;    
}    
namespace N2     
{    
int a= 2;    
}    
void fun()    
{  
int a = 2;
cout<<"Fun A : "<< a <<" "<<::b<<endl; 
}   
int main() 
{    
system(“color  F0”);    
cout << "Roll No: 24B11AI330" << endl;    
    {            
int a = 2;  // Inner block variable         
 cout << "Inner A : " << a << endl;    
    }    
cout << "Outer B : " << b << endl;       
fun();   
cout <<  "Name space 1 : " << N1::a << endl;    
cout <<  "Name space 2 : " << N2::a << endl;    
    return 0;    
}   
