#include<iostream> 
#include<cstdlib> 
using namespace std; 
inline int square(intx)  
{      
    return x * x;    
} 
 int main()     
{     
 system(“color F0”);       
cout<<"Roll No: 24B11AI330" << endl;       
int num = 4; 
cout << "Square of " << num << " is " << square(num) << endl; 
 return 0;    
} 
